package com.teak{	import flash.display.Sprite;	import flash.events.MouseEvent;	import flash.display.Bitmap;		import com.greensock.TweenNano;	import com.greensock.easing.Linear;	import com.greensock.easing.Expo;		import com.doubleclick.studio.Enabler;		public class HPBeats728x90 extends Sprite	{		public function HPBeats728x90():void		{			Enabler.init(stage);						this.buttonMode = true;			beatsOnly.visible = false;			hearLine.visible = false;			likeLine.visible = false;			endCopy.visible = false;			hpLogo.visible = false;			cta.visible = false;			animate1();						stage.addEventListener(MouseEvent.CLICK, bannerClicked);		}		private function animate1():void		{			TweenNano.to(laptop, 6.5, {delay:0,scaleX:1.1,scaleY:1.1, ease:Linear.easeNone,onComplete:animate3});						TweenNano.from(rockBack, .75, {delay:2, alpha:0});			TweenNano.to(rockBack, .75, {delay:3.25, alpha:0, overwrite:false,onComplete:animate2});		}		private function animate2():void		{			beatsOnly.visible = true;			beatsOnly.alpha = 0;			TweenNano.to(beatsOnly, .75, {alpha:1});			TweenNano.to(beatsOnly, .75, {delay:2, overwrite:false,alpha:0});		}		private function animate3():void		{			TweenNano.to(laptop.crowd, .25, {delay:.25,alpha:0});			TweenNano.to(laptop, 1, {delay:0,scaleX:.12, scaleY:.12,x:laptop.x - 15,ease:Expo.easeOut,onComplete:sharpCPU});						hearLine.visible = true;						hpLogo.visible = true;			hpLogo.alpha = 0;			TweenNano.to(hpLogo, .75, {delay:1, alpha:1});						TweenNano.to(hearLine, .25, {delay:2, alpha:0, onComplete:animate4})		}		private function sharpCPU():void		{			laptop.laptopArt.visible = false;						var laptopArt:LaptopArt = new LaptopArt();			addChildAt(laptopArt,1);			laptopArt.x = 178;			laptopArt.y = 6;						/*laptop.x = 223;			laptop.y = 43;			laptop.width = 116;			laptop.height = 82;			*/			trace(laptop.x, laptop.y, laptop.width,laptop.height,laptop.scaleX,laptop.scaleY);			//trace(laptop.laptopArt.x, laptop.laptopArt.y, laptop.laptopArt.width,laptop.laptopArt.height,laptop.laptopArt.scaleX,laptop.laptopArt.scaleY);*/		}		private function animate4():void		{			likeLine.visible = true;			likeLine.alpha = 0;			TweenNano.to(likeLine, .75, {alpha:1});			TweenNano.to(likeLine, .75, {delay:2,alpha:0, overwrite:false,onComplete:animate5});		}		private function animate5():void		{			endCopy.visible = true;			TweenNano.from(endCopy, .75, {alpha:0});						cta.visible = true;			cta.alpha = 0;			TweenNano.to(cta, .75, {delay:2, alpha:1});		}		private function bannerClicked(event:MouseEvent):void		{			Enabler.exit("banner_clicked");		}	}}