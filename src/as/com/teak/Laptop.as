package com.teak{	import flash.display.Sprite;		import com.greensock.TweenNano;		public class Laptop extends Sprite	{		public function Laptop():void		{			var flashes:Flashes = new Flashes(flashArea.x,flashArea.y, flashArea.width, flashArea.height);			addChild(flashes);						higher1.alpha = higher2.alpha = higher3.alpha = .4;						TweenNano.to(lightPools, .5, {delay:14.5, alpha:0});			TweenNano.delayedCall(15,lightPools.stop);			TweenNano.delayedCall(15,flashes.stopFlashes);		}	}}